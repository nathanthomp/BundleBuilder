<UserControl x:Class="RedBuilt.Revit.BundleBuilder.Views.ExportView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RedBuilt.Revit.BundleBuilder.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TextBlock Text="Export View"/>
        <Button Content="Cancel" Command="{Binding NavigateBundleCommand}" Margin="0,0,0,23" Height="36" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="308"/>
        <Button Content="Export" Command="{Binding ExportCommand}" Margin="0,0,0,23" Height="36" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="308"/>
    </Grid>
</UserControl>
